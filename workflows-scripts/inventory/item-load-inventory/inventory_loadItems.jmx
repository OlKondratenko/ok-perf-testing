                <stringProp name="cacheKey">true</stringProp>
                <stringProp name="script">import org.apache.jmeter.assertions.AssertionResult;
import org.apache.jmeter.util.JMeterUtils;
import java.text.SimpleDateFormat;
import groovy.json.JsonBuilder
import groovy.json.JsonSlurper

// #1 Create `loanDate`
SimpleDateFormat dateFormatGmt = new SimpleDateFormat(&quot;yyyy-MM-dd&apos;T&apos;HH:mm:ss.SSSZ&quot;);
dateFormatGmt.setTimeZone(TimeZone.getTimeZone(&quot;UTC&quot;));

def currentDateWithShift = new Date(Calendar.getInstance().getTimeInMillis() + (1 * 60000));
def loanDate = dateFormatGmt.format(currentDateWithShift).replace(&quot;+0000&quot;, &quot;Z&quot;);
JMeterUtils.setProperty(&quot;loanDate&quot;, loanDate);


// #2 Defind `itemBarcodes`
def itemBarcodesStr = JMeterUtils.getProperty(&quot;itemBarcodesJson&quot;);
def itemBarcodesJson = new JsonSlurper().parseText(itemBarcodesStr);                       
def ids = itemBarcodesJson.itemBarcodeIds;
log.info(&quot;Available item barcodes before checkout &quot;+ ids);

def id = ids[0]
if(id == null) {
	def errorMsg = &quot;There are no available items in the system!&quot;;
	log.error(errorMsg);
     AssertionResult assertionResult = new AssertionResult(&quot;AssertionResult&quot;);
	assertionResult.setFailureMessage(errorMsg);
     assertionResult.setFailure(true); 
} else {
	JMeterUtils.setProperty(&quot;itemBarcode&quot;, id);
	log.info(&quot;itemBarcode for check-out: &quot; + id);		
}

itemBarcodesJson.itemBarcodeIds.remove(id); //remove(itemBarcode);          

def jsonBuilder = new groovy.json.JsonBuilder(); // rewrite itemBarcodeIds to json
jsonBuilder itemBarcodeIds:  itemBarcodesJson.itemBarcodeIds                                                   

JMeterUtils.setProperty(&quot;itemBarcodesJson&quot;, jsonBuilder.toString());

// #3 Create `checkInItemBarcodes` for check-in
def checkInItemBarcodesStr = JMeterUtils.getProperty(&quot;checkInItemBarcodes&quot;);

List&lt;String&gt; checkInItemBarcodes;
if(checkInItemBarcodesStr == null){
 	checkInItemBarcodes = new ArrayList&lt;&gt;();
}else {
  	checkInItemBarcodes = checkInItemBarcodesStr.replace(&quot;[&quot;,&quot;&quot;).replace(&quot;]&quot;,&quot;&quot;).split(&apos;,&apos;).collect{it.trim() as String};
  	def first = checkInItemBarcodes[0];
  	if(!first?.trim()){
  		checkInItemBarcodes.remove(first);
  	}
}

if(id != null){
	checkInItemBarcodes.add(id); // add itemBarcode id 
}

JMeterUtils.setProperty(&quot;checkInItemBarcodes&quot;, checkInItemBarcodes.toString());
log.info(&quot;List of items for check-in: &quot; + checkInItemBarcodes.toString());
JMeterUtils.setProperty(&quot;distribution&quot;, &quot;1&quot;);</stringProp>
