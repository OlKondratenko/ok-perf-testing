              <stringProp name="cacheKey">true</stringProp>
              <stringProp name="script">import org.apache.jmeter.assertions.AssertionResult;
import org.apache.jmeter.util.JMeterUtils;
import java.text.SimpleDateFormat;
import groovy.json.JsonSlurper

// #1 Create `checkInDate`
SimpleDateFormat dateFormatGmt = new SimpleDateFormat(&quot;yyyy-MM-dd&apos;T&apos;HH:mm:ss.SSSZ&quot;);
dateFormatGmt.setTimeZone(TimeZone.getTimeZone(&quot;UTC&quot;));
def checkInDate = dateFormatGmt.format(new Date()).replace(&quot;+0000&quot;, &quot;Z&quot;);

JMeterUtils.setProperty(&quot;checkInDate&quot;, checkInDate);

// #2 Defind `checkInItemBarcodes`
def checkInItemBarcodesStr = JMeterUtils.getProperty(&quot;checkInItemBarcodes&quot;);

List&lt;String&gt; checkInItemBarcodes;
if(checkInItemBarcodesStr == null){
 	checkInItemBarcodes = new ArrayList&lt;&gt;();
} else {
  	checkInItemBarcodes = checkInItemBarcodesStr.replace(&quot;[&quot;,&quot;&quot;).replace(&quot;]&quot;,&quot;&quot;).split(&apos;,&apos;).collect{it.trim() as String};
}

def id = checkInItemBarcodes[0];
if(id == null){
 	def errorMsg = &quot;Not defined item id!&quot;;
	log.error(errorMsg);
     AssertionResult assertionResult = new AssertionResult(&quot;AssertionResult&quot;);
	assertionResult.setFailureMessage(errorMsg);
     assertionResult.setFailure(true); 
}

checkInItemBarcodes.remove(id);

